			  tickLed1=HAL_GetTick();
		  }
		  LED=led2;
		  break;
	  case led2:
		  if((HAL_GetTick()-tickLed2) >= 500){
			  HAL_GPIO_TogglePin(amberLed_GPIO_Port, amberLed_Pin);
